{
  "name": "@eamcet-platform/ai-worker",
  "version": "1.0.0",
  "description": "AI Question Generator Cloudflare Worker",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:prod": "wrangler deploy --env production",
    "build": "tsc",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:pbt": "vitest --run --reporter=verbose",
    "test:coverage": "vitest --run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@eamcet-platform/shared": "*",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230710.0",
    "wrangler": "^3.5.0",
    "vitest": "^0.34.0",
    "fast-check": "^3.12.0",
    "@vitest/coverage-v8": "^0.34.0",
    "jest": "^29.6.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}
