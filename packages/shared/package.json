{
  "name": "@eamcet-platform/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for EAMCET Mock Test Platform",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:pbt": "vitest --run --reporter=verbose **/*.pbt.test.ts",
    "test:coverage": "vitest --run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "vitest": "^0.34.0",
    "fast-check": "^3.12.0",
    "@vitest/coverage-v8": "^0.34.0",
    "jest": "^29.6.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}
